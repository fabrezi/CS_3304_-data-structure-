//simple example of an important concept

#include <stdio.h>
#include <iostream>
using namespace std;

void swapnum(int a, int b){
    int temp = a;
    a = b;
    b = temp;
}

void swapnum2(int &a, int &b){
    int temp = a;
    a = b;
    b = temp;
}

int main(){
    int a = 10;
    int b = 7;
    
    swapnum(a,b);
    cout<< "a " << a <<endl;
    cout << "b " << b << endl;
    
swapnum2(a,b);
cout<< "a " << a <<endl;
cout<< "b " << b <<endl;

}
